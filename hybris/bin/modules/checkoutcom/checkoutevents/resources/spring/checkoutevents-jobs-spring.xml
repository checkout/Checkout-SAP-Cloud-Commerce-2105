<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <alias name="defaultCheckoutComPaymentEventProcessingJob" alias="checkoutComPaymentEventProcessingJob"/>
    <bean id="defaultCheckoutComPaymentEventProcessingJob"
          class="com.checkout.hybris.events.cronjob.performables.CheckoutComPaymentEventProcessingJob"
          parent="abstractJobPerformable">
        <constructor-arg ref="checkoutComPaymentEventDao"/>
        <constructor-arg ref="checkoutComPaymentEventProcessingService"/>
    </bean>

    <alias name="defaultCheckoutComPaymentEventCleanupJob" alias="checkoutComPaymentEventCleanupJob"/>
    <bean id="defaultCheckoutComPaymentEventCleanupJob"
          class="com.checkout.hybris.events.cronjob.performables.CheckoutComPaymentEventCleanupJob"
          parent="abstractJobPerformable">
        <constructor-arg ref="checkoutComPaymentEventCleanupService"/>
    </bean>

    <alias name="defaultCheckoutComCleanupJob" alias="checkoutComCleanupJob"/>
    <bean id="defaultCheckoutComCleanupJob"
          class="com.checkout.hybris.events.cronjob.performables.CheckoutComCleanupJob"
          parent="abstractJobPerformable">
        <constructor-arg name="checkoutComCleanupService" ref="checkoutComCleanupService"/>
    </bean>

</beans>
