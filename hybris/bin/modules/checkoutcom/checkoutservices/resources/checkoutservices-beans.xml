                  type="java.util.List&lt;com.checkout.hybris.core.klarna.session.response.KlarnaPaymentMethodCategoryDto&gt;">
            <annotations>@JsonProperty("payment_method_categories")</annotations>
        </property>
        <property name="links" type="java.util.Map&lt;java.lang.String,java.lang.Object&gt;">
            <annotations>@JsonProperty("_links")</annotations>
